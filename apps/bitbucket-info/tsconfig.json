{
  "compilerOptions": {
    "target": "es2022",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@core-utils": ["libs/core-utils/src/index.ts"],
      "@ui-kit": ["libs/ui-kit/src/index.ts"],
      "@charts": ["libs/charts/src/index.ts"],
      "@pages/*": ["apps/bitbucket-info/src/app/pages/*"],
      "@enums/*": ["apps/bitbucket-info/src/app/enums/*"],
      "@api/*": ["apps/bitbucket-info/src/app/api/*"],
      "@services/*": ["apps/bitbucket-info/src/app/services/*"],
      "@globalTypes/*": ["apps/bitbucket-info/src/app/types/*"],
      "@environment/*": ["apps/bitbucket-info/src/environments/*"]
    }
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.editor.json"
    },
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ],
  "extends": "../../tsconfig.base.json",
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
